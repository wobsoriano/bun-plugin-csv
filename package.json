{
  "name": "bun-plugin-csv",
  "version": "0.1.1",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "description": "A Bun plugin which converts .csv and .tsv files into JavaScript modules.",
  "scripts": {
    "build": "bun run build.mjs",
    "prepublishOnly": "bun run build",
    "release": "bumpp && npm publish"
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "keywords": [
    "bun"
  ],
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/bun-plugin-csv#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/bun-plugin-csv.git"
  },
  "bugs": "https://github.com/wobsoriano/bun-plugin-csv/issues",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "devDependencies": {
    "@types/d3-dsv": "^3.0.2",
    "bumpp": "^9.2.0",
    "bun-plugin-dts": "^0.1.0",
    "bun-types": "^1.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "d3-dsv": "^3.0.1"
  }
}
